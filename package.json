{
  "name": "lavamusic",
  "version": "5.0.0-beta",
  "description": "LavaMusic is a music bot for Discord, written in JavaScript using the Discord.js, Typescript, lavalink-client (Lavalink) library.",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run clean && node .",
    "dev": "nodemon --watch src/ --exec npm run start -e ts",
    "db:push": "npx prisma db push",
    "db:push:new": "npx prisma db push --schema=./prisma/schema.new.prisma",
    "db:migrate": "npx prisma migrate dev --name init",
    "build": "tsc --project tsconfig.json && tsc-alias",
    "clean": "node scripts/clean.js && npm run build",
    "lint": "biome lint --write",
    "format": "biome format --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appujet/lavamusic.git"
  },
  "keywords": [
    "discord",
    "music",
    "bot",
    "lavalink",
    "lavalink-client",
    "lavamusic",
    "typescript",
    "prisma"
  ],
  "author": "appujet",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/appujet/lavamusic/issues"
  },
  "homepage": "https://github.com/appujet/lavamusic#readme",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@biomejs/biome": "^1.9.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/i18n": "^0.13.12",
    "@types/node": "^22.8.7",
    "@types/signale": "^1.4.7",
    "prisma": "^5.20.0",
    "tsc-alias": "^1.8.10",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "@top-gg/sdk": "^3.1.6",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "express": "^5.0.0",
    "genius-lyrics-api": "^3.2.1",
    "i18n": "^0.15.1",
    "lavalink-client": "github:tomato6966/lavalink-client",
    "node-system-stats": "^1.3.0",
    "signale": "^1.4.0",
    "socket.io": "^4.8.0",
    "topgg-autoposter": "^2.0.2",
    "undici": "^6.20.1",
    "zod": "^3.23.8"
  },
  "signale": {
    "displayScope": true,
    "displayBadge": true,
    "displayDate": true,
    "displayFilename": true,
    "displayLabel": true,
    "displayTimestamp": true,
    "underlineLabel": true
  }
}
